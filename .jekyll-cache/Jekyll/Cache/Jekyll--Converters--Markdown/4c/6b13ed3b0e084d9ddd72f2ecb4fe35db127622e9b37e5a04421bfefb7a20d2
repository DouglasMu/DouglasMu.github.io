    class Solution:
        def isValid(self,s):
          stack = []
          map = {
            â€œ{â€œ:â€}â€,
            â€œ[â€:â€]â€,
            â€œ(â€œ:â€)â€
          }
          for x in s:
            if x in map:
              stack.append(map[x])
            else:
              if len(stack)!=0:
                top_element = stack.pop()
                if x != top_element:
                  return False
                else:
                  continue
              else:
                return False
          return len(stack) == 0</p>

